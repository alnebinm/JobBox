.section-box
    .container 
        .panel-white.mb-30
            .box-padding
                h5 Update your CV
                .row.mt-30
                    .col-lg-6.col-md-6
                        .form-group.mb-30
                            .box-upload
                                .add-file-upload
                                    input.fileupload(type="file", name='file')
                                a.btn.btn-default Upload File
                    .col-lg-6.col-md-6
                        .form-group.mb-30.d-flex.align-items-center.box-file-uploaded
                            span Job_required.pdf
                            a.btn.btn-delete Delete
        .panel-white.mb-30
            .box-padding
                h5.icon-edu Education
                .row.mt-30
                    .col-lg-9
                        .row
                            .col-lg-6.col-md-6
                                .form-group.mb-30 
                                    label.font-sm.color-text-mutted.mb-10 From
                                    input.form-control(type='date', data-date-format="DD MMMM YYYY", data-date="17 September 2022", value="2022-09-20", placeholder='12 September 2008')
                            .col-lg-6.col-md-6
                                .form-group.mb-30 
                                    label.font-sm.color-text-mutted.mb-10 To
                                    input.form-control(type='date', data-date-format="DD MMMM YYYY", data-date="17 September 2022", value="2022-09-20", placeholder='12 September 2008')
                            .col-lg-12 
                                .form-group.mb-30 
                                    label.font-sm.color-text-mutted.mb-10 Description
